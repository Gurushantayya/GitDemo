package com.qa.ArrayandString;

public class Test1 {

	public static void main(String[] args) {
		
		
		
		String s= new String("mallikarjun");
		s.concat("Akshay");
		System.out.println(s);
		
		
		
		StringBuffer sb= new StringBuffer("mallikarjun");
		sb.append("akshay");
		System.out.println(sb);
		

		
		
		
		
		
	}

}
