package com.qa.ArrayandString;

public class Test1 {

	public static void main(String[] args) {
		
		
		
		String s= "mallikarjun";
		String s1="akshay";
		System.out.println(s.concat(s1));
		
		
		
		
		StringBuffer sb= new StringBuffer("mallikarjun");
		sb.append("akshay");
		System.out.println(sb);
		

		
		
		
		
		
	}

}
